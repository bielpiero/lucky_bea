<robot>
	<navigation-params alpha="70">
		<initial-position>
			<x-zone x1="-15" x2="-10" x3="10" x4="15"/> <!-- centimeters -->
			<y-zone x1="-20" x2="-15" x3="15" x4="20"/> <!-- centimeters -->
			<th-zone x1="-110" x2="-60" x3="60" x4="110"/> <!-- milliradians -->
		</initial-position>
		<process-noise>
			<x-zone x1="-10" x2="-5" x3="5" x4="10"/> <!-- centimeters -->
			<y-zone x1="-10" x2="-5" x3="5" x4="10"/> <!-- centimeters -->
			<th-zone x1="-100" x2="-80" x3="80" x4="100"/> <!-- milliradians -->
		</process-noise>
		<observation-noise mode="d-th">
			<d-zone x1="-50" x2="-25" x3="25" x4="50"/>  <!-- millimeters -->
			<th-zone x1="-340" x2="-300" x3="300" x4="340"/> <!-- milliradians -->
		</observation-noise>
	</navigation-params>
</robot>