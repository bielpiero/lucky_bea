<robot>
	<navigation-params alpha="70">
		<initial-position>
			<x-zone x1="-50" x2="-40" x3="40" x4="50"/> <!-- centimeters -->
			<y-zone x1="-20" x2="-17" x3="17" x4="20"/> <!-- centimeters -->
			<th-zone x1="-30" x2="-20" x3="20" x4="30"/> <!-- milliradians -->
		</initial-position>
		<process-noise>
			<x-zone x1="-15" x2="-10" x3="10" x4="15"/>
			<y-zone x1="-2" x2="-1" x3="1" x4="2"/>
			<th-zone x1="-15" x2="-10" x3="10" x4="15"/> <!-- milliradians -->
		</process-noise>
		<observation-noise mode="d-th">
			<d-zone x1="-50" x2="-30" x3="30" x4="50"/>
			<th-zone x1="-100" x2="-85" x3="85" x4="100"/> <!-- milliradians -->
		</observation-noise>
	</navigation-params>
</robot>